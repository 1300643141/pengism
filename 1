		jQuery(function(){
			var t;
			jQuery(".morediv").hover(
				function(){
					t = window.setTimeout("jQuery('#morelist').show()",500);				
				}
				,
				function(){
					jQuery('#morelist').hide();
					t = window.clearTimeout(t);
				}
			);
		});

