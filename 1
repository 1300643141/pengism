			jQuery("#isXy").css("display","");
			jQuery("#div_isXy").css("display","none");
		} else {
			jQuery("#isXx").css("display","");
			jQuery("#div_isXx").css("display","none");
		}

		if (jsCou <= 1){
			jQuery("#qxqh").css("display","none");
		}
	})
