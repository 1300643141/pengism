		if (fdyQx=="true" && bzrQx=="true"){
			jQuery("#isJd").css("display","");
			jQuery("#div_isJd").css("display","none");
		} else if(fdyQx=="true"){
			jQuery("#isFdy").css("display","");
			jQuery('#div_isFdy').css("display","none");
		} else if(bzrQx=="true"){
			jQuery("#isBzr").css("display","");
			jQuery('#div_isBzr').css("display","none");
